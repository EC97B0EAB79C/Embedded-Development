<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ToggleButton
        android:id="@+id/toggle_button_led_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ToggleButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/toggle_button_led_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ToggleButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toggle_button_led_1" />

    <ToggleButton
        android:id="@+id/toggle_button_led_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ToggleButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toggle_button_led_2" />

    <ToggleButton
        android:id="@+id/toggle_button_led_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ToggleButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toggle_button_led_3" />

    <ToggleButton
        android:id="@+id/toggle_button_led_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ToggleButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toggle_button_led_4" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LED1"
        app:layout_constraintBottom_toBottomOf="@+id/toggle_button_led_1"
        app:layout_constraintEnd_toStartOf="@+id/toggle_button_led_1"
        app:layout_constraintTop_toTopOf="@+id/toggle_button_led_1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LED2"
        app:layout_constraintBottom_toBottomOf="@+id/toggle_button_led_2"
        app:layout_constraintEnd_toStartOf="@+id/toggle_button_led_2"
        app:layout_constraintTop_toTopOf="@+id/toggle_button_led_2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LED3"
        app:layout_constraintBottom_toBottomOf="@+id/toggle_button_led_3"
        app:layout_constraintEnd_toStartOf="@+id/toggle_button_led_3"
        app:layout_constraintTop_toTopOf="@+id/toggle_button_led_3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LED4"
        app:layout_constraintBottom_toBottomOf="@+id/toggle_button_led_4"
        app:layout_constraintEnd_toStartOf="@+id/toggle_button_led_4"
        app:layout_constraintTop_toTopOf="@+id/toggle_button_led_4" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LED5"
        app:layout_constraintBottom_toBottomOf="@+id/toggle_button_led_5"
        app:layout_constraintEnd_toStartOf="@+id/toggle_button_led_5"
        app:layout_constraintTop_toTopOf="@+id/toggle_button_led_5" />
</androidx.constraintlayout.widget.ConstraintLayout>