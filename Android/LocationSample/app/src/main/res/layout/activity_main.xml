<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/gps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GPS"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/gps_latitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="latitude"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gps" />
    <TextView
        android:id="@+id/gps_longitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="longitude"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gps_latitude" />
    <TextView
        android:id="@+id/gps_accuracy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="accuracy"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gps_longitude" />
    <TextView
        android:id="@+id/gps_altitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="altitude"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gps_accuracy" />

    <TextView
        android:id="@+id/wifi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WiFi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gps_altitude" />
    <TextView
        android:id="@+id/wifi_latitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="latitude"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wifi" />
    <TextView
        android:id="@+id/wifi_longitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="longitude"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wifi_latitude" />
    <TextView
        android:id="@+id/wifi_accuracy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="accuracy"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wifi_longitude" />

    <TextView
        android:id="@+id/wifi_altitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="altitude"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wifi_accuracy" />

    <TextView
        android:id="@+id/text_view_gps_latitude_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/gps_latitude"
        tools:text="69" />
    <TextView
        android:id="@+id/text_view_gps_longitude_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/gps_longitude"
        tools:text="69" />
    <TextView
        android:id="@+id/text_view_gps_accuracy_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/gps_accuracy"
        tools:text="69" />
    <TextView
        android:id="@+id/text_view_gps_altitude_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/gps_altitude"
        tools:text="69" />

    <TextView
        android:id="@+id/text_view_wifi_latitude_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/wifi_latitude"
        tools:text="69" />
    <TextView
        android:id="@+id/text_view_wifi_longitude_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/wifi_longitude"
        tools:text="69" />
    <TextView
        android:id="@+id/text_view_wifi_accuracy_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/wifi_accuracy"
        tools:text="69" />
    <TextView
        android:id="@+id/text_view_wifi_altitude_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/wifi_altitude"
        tools:text="69" />

    <Button
        android:id="@+id/button_gps"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="GPS"
        app:layout_constraintEnd_toStartOf="@+id/button_wifi"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wifi_altitude" />

    <Button
        android:id="@+id/button_wifi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="WiFi"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_gps"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button_gps" />

</androidx.constraintlayout.widget.ConstraintLayout>